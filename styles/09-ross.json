{
  "$schema": "https://schemas.wp.org/wp/6.7/theme.json",
  "version": 3,
  "title": "Ross",
  "settings": {
    "color": {
      "duotone": [
        {
          "colors": ["#4433A6", "#79F3B1"],
          "name": "Midnight filter",
          "slug": "midnight-filter"
        }
      ],
      "palette": [
        {
          "color": "#FFFFFF",
          "name": "Base",
          "slug": "base"
        },
        {
          "color": "#000000",
          "name": "Contrast",
          "slug": "contrast"
        },
        {
          "color": "#E30B2E",
          "name": "Accent 1",
          "slug": "accent-1"
        },
        {
          "color": "#F6F6F6",
          "name": "Accent 2",
          "slug": "accent-2"
        },
        {
          "color": "#D1D0EA",
          "name": "Accent 3",
          "slug": "accent-3"
        },
        {
          "color": "#CBCBCB",
          "name": "Accent 4",
          "slug": "accent-4"
        },
        {
          "color": "#353535",
          "name": "Accent 5",
          "slug": "accent-5"
        },
        {
          "color": "#FFFFFF33",
          "name": "Accent 6",
          "slug": "accent-6"
        }
      ]
    },
    "typography": {
      "fontSizes": [
        {
          "fluid": false,
          "name": "Small",
          "size": "0.9rem",
          "slug": "small"
        },
        {
          "fluid": {
            "max": "1rem",
            "min": "0.75rem"
          },
          "name": "Medium",
          "size": "clamp(0.75rem, 0.7rem + 0.25vw, 1rem)",
          "slug": "medium"
        },
        {
          "fluid": {
            "max": "1.8rem",
            "min": "1.2rem"
          },
          "name": "Large",
          "size": "1.2rem",
          "slug": "large"
        },
        {
          "fluid": {
            "max": "2.2rem",
            "min": "1.8rem"
          },
          "name": "Extra Large",
          "size": "1.8rem",
          "slug": "x-large"
        },
        {
          "fluid": {
            "max": "2.75rem",
            "min": "1.625rem"
          },
          "name": "Extra Extra Large",
          "size": "clamp(1.625rem, 1.4rem + 1.125vw, 2.75rem)",
          "slug": "xx-large"
        }
      ],
      "fontFamilies": [
        {
          "name": "Roboto",
          "slug": "roboto",
          "fontFamily": "\"Roboto\", serif",
          "fontFace": [
            {
              "fontFamily": "\"Roboto\"",
              "fontStyle": "normal",
              "fontWeight": "100 900",
              "src": [
                "file:./assets/fonts/roboto/Roboto-VariableFont_wdth,wght.ttf"
              ]
            }
          ]
        },
        {
          "name": "Montserrat",
          "slug": "montserrat",
          "fontFamily": "\"Montserrat\", serif",
          "fontFace": [
            {
              "fontFamily": "\"Montserrat\"",
              "fontStyle": "normal",
              "fontWeight": "100 900",
              "src": [
                "file:./assets/fonts/montserrat/Montserrat-VariableFont_wght.ttf"
              ]
            }
          ]
        }
      ]
    }
  },
  "styles": {
    "typography": {
      "fontSize": "var:preset|font-size|medium",
      "fontFamily": "var:preset|font-family|roboto",
      "lineHeight": "1.2"
    },
    "blocks": {
      "core/avatar": {
        "filter": {
          "duotone": "var:preset|duotone|midnight-filter"
        }
      },
      "core/code": {
        "color": {
          "background": "var:preset|color|accent-2",
          "text": "var:preset|color|contrast"
        }
      },
      "core/post-date": {
        "color": {
          "text": "var:preset|color|contrast"
        },
        "elements": {
          "link": {
            "color": {
              "text": "var:preset|color|contrast"
            }
          }
        }
      },
      "core/post-title": {
        "typography": {
          "fontWeight": "200"
        }
      },
      "core/pullquote": {
        "border": {
          "bottom": {
            "style": "none",
            "width": "0px"
          },
          "left": {
            "style": "none",
            "width": "0px"
          },
          "right": {
            "style": "none",
            "width": "0px"
          },
          "top": {
            "style": "none",
            "width": "0px"
          }
        },
        "typography": {
          "fontFamily": "var:preset|font-family|roboto",
          "fontWeight": "200",
          "letterSpacing": "0em"
        }
      },
      "core/query-pagination": {
        "typography": {
          "fontWeight": "300",
          "letterSpacing": "0px"
        }
      },
      "core/quote": {
        "typography": {
          "fontSize": "var:preset|font-size|medium",
          "letterSpacing": "-0.01em",
          "lineHeight": "1.5",
          "fontWeight": "300"
        }
      },
      "core/search": {
        "border": {
          "radius": "0px"
        }
      },
      "core/site-title": {
        "typography": {
          "fontFamily": "var:preset|font-family|roboto",
          "fontSize": "var:preset|font-size|x-large",
          "fontWeight": "300",
          "letterSpacing": "-0.56px",
          "textTransform": "uppercase"
        }
      }
    },
    "elements": {
      "button": {
        "border": {
          "radius": "2px"
        },
        "color": {
          "background": "var:preset|color|accent-1",
          "text": "var:preset|color|base"
        },
        "typography": {
          "fontFamily": "var:preset|font-family|roboto",
          "fontSize": "var:preset|font-size|medium",
          "fontWeight": "600",
          "textTransform": "uppercase"
        },
        ":hover": {
          "color": {
            "background": "color-mix(in srgb, var(--wp--preset--color--accent-1) 85%, transparent)"
          }
        }
      },
      "heading": {
        "typography": {
          "fontFamily": "var:preset|font-family|roboto",
          "fontWeight": "200",
          "letterSpacing": "-0.02em",
          "lineHeight": "1.24"
        }
      },
      "h6": {
        "typography": {
          "fontWeight": "200"
        }
      },
      "h1": {
        "typography": {
          "fontSize": "var:preset|font-size|xx-large",
          "fontWeight": "500"
        }
      }
    },
    "variations": {
      "section-1": {
        "color": {
          "text": "var:preset|color|accent-3"
        },
        "elements": {
          "button": {
            "color": {
              "background": "var:preset|color|accent-3",
              "text": "var:preset|color|accent-5"
            },
            ":hover": {
              "color": {
                "background": "color-mix(in srgb, var(--wp--preset--color--accent-3) 85%, transparent)"
              }
            }
          },
          "link": {
            "color": {
              "text": "currentColor"
            }
          }
        }
      },
      "section-4": {
        "color": {
          "text": "var:preset|color|accent-5"
        },
        "elements": {
          "button": {
            "color": {
              "background": "var:preset|color|accent-5",
              "text": "var:preset|color|accent-3"
            },
            ":hover": {
              "color": {
                "background": "color-mix(in srgb, var(--wp--preset--color--accent-5) 85%, transparent)"
              }
            }
          },
          "link": {
            "color": {
              "text": "currentColor"
            }
          }
        }
      }
    }
  }
}
